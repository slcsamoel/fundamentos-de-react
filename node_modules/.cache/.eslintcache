2>O número é {props.numero}</h2>\n            <If  test={props.numero % 2 === 0}>\n                <span>Par</span>\n            </If>   \n\n            <If  test={props.numero % 2 === 1}>\n                <span>Impar</span>\n            </If>   \n        </div>\n    );\n\n}\n","/app/www/samoel/react-exercicios/fundamentos-react/src/components/basicos/If.jsx",["150"],"// nesse componente não é necessario importar o react, pois não a sintax jsx apenas java script puro \nexport default function (props){\n    if(props.test){\n        return props.children\n    }else{\n        return false;\n    }\n    \n}","/app/www/samoel/react-exercicios/fundamentos-react/src/components/comunicacao/direta/Pai.jsx",["151"],"import React from 'react';\n\nimport Filho from './Filho';\n\nexport default props =>\n    <div>\n        <Filho sobrenome=\"Silva\">João</Filho>\n        <Filho sobrenome=\"Silva\">Maria</Filho>\n        <Filho sobrenome=\"Silva\">Pedro</Filho>\n    </div>","/app/www/samoel/react-exercicios/fundamentos-react/src/components/comunicacao/direta/Filho.jsx",["152"],"import React from 'react';\n\n\nexport default props =>\n    <div>\n       <h3>Componente Filho</h3>\n       <h4>{props.children} - {props.sobrenome}</h4>\n    </div>","/app/www/samoel/react-exercicios/fundamentos-react/src/components/animacao/Animation.jsx",["153"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/index.js",[],["154","155"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/App.jsx",["156"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/Primeiro.jsx",[],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/ComParamentro.jsx",["157"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/ComFilhos.jsx",["158"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/Repeticao.jsx",["159"],"/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/Condicional