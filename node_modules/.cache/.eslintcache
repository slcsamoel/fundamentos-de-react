eturn <li key={prod.id}>{prod.id } - {prod.nome} - R${prod.preco} </li>;\n            });\n    }\n\n    return (\n        <div>\n              <h2>Repetição</h2>  \n              <ul>\n                  {getProdutosListItem()}\n              </ul>\n        </div>\n    );\n\n}","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/Condicional.jsx",["110"],"import React from 'react';\n\nexport default props => {\n\n\n    return (\n        <div>\n            <h2>O número é {props.numero}</h2>\n            {props.numero % 2 === 0 ? <span>Par</span> : <span>Impar</span> }\n        </div>\n    );\n\n}\n","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/CondicionalComIf.jsx",["111"],"import React from 'react';\nimport If from './If';\n\nexport default props => {\n\n\n    return (\n        <div>\n            <h2>O número é {props.numero}</h2>\n            <If  test={props.numero % 2 === 0}>\n                <span>Par</span>\n            </If>   \n\n            <If  test={props.numero % 2 === 1}>\n                <span>Impar</span>\n            </If>   \n        </div>\n    );\n\n}\n","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/layout/Card.jsx",["112"],"import React from 'react';\nimport './Card.css';\n\nexport default props =>\n    <div className=\"Card\" style={{\n             borderColor: props.color || '#000'\n        }}>\n\n         <div className=\"Content\" style={{\n             backgroundColor: props.contentColor || '#fff'\n         }}>\n            {props.children}\n        </div>\n\n        <div className=\"Footer\" style={{\n            backgroundColor: props.color || '#000'\n            }}>\n            {props.titulo}\n        </div> \n\n    </div>","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/animacao/Animation.jsx",["113"],"\nimport React, {useEffect , useRef } from 'react';\nimport lottie from 'lottie-web';\n\nfunction  Animation(props) {\n\n    const container = useRef(null)\n\n    useEffect(()=>{\n       lottie.loadAnimation({\n           container: container.current,\n           renderer: 'svg',\n           loop: true ,\n           autoplay: true ,\n           animationData: require(`./${props.data}`)\n       })\n     },[])\n\n    return (\n        <div className=\"container\" ref={container}></div>\n    );\n\n}\n\nexport default Animation;","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/comunicacao/direta/Pai.jsx",["114"],"import React from 'react';\n\nimport Filho from './Filho';\n\nexport default props =>\n    <div>\n        <Filho {...props}>João</Filho>\n        <Filho sobrenome={props.sobrenome}>Maria</Filho>\n        <Filho sobrenome=\"Silva\">Pedro</Filho>\n    </div>","/app/www/samoel/react-exercicios/fundamentos-de-react/src/data/produtos.js",["115"],"export default [\n    { id:1 , nome: 'Caneta' , preco: 7.59},\n    { id:2 , nome: 'Lapis' , preco: 3.89},\n    { id:3 , nome: 'Caderno' , preco: 18.30},    \n]","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/basicos/If.jsx",["116"],"// nesse componente não é necessario importar o react, pois não a sintax jsx apenas java script puro \nexport default function (props){\n    if(props.test){\n        return props.children\n    }else{\n        return false;\n    }\n    \n}","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/comunicacao/direta/Filho.jsx",["117"],"import React from 'react';\n\n\nexport default props =>\n    <div>\n       <h3>Componente Filho</h3>\n       <h4>{props.children} - {props.sobrenome}</h4>\n    </div>","/app/www/samoel/react-exercicios/fundamentos-de-react/src/components/comunicacao/indireta/Super.jsx",["118"],"import React, { useState } from 'react';\nimport Sub from './Sub';\n\nexport default (props) => {\n\n    const [texto,setTexto] = useState('Valor')\n    const [num,setNum] = useState(0) \n\n    function qndClicar(valorGerado , texto){\n        setNum(valorGerado);\n   